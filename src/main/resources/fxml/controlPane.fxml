<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mp3player.controller.ControlPaneController">
   <Button id="prev" fx:id="previousButton" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER">
      <padding>
         <Insets left="10.0" right="10.0" />
      </padding></Button>
    <ToggleButton id="play" fx:id="playButton" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER">
      <padding>
         <Insets left="10.0" right="10.0" />
      </padding></ToggleButton>
    <Button id="next" fx:id="nextButton" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" HBox.hgrow="NEVER">
      <padding>
         <Insets left="10.0" right="10.0" />
      </padding></Button>
    <Slider fx:id="volumeSlider" HBox.hgrow="NEVER">
      <padding>
         <Insets left="10.0" right="10.0" />
      </padding></Slider>
    <Slider fx:id="progressSlider" HBox.hgrow="ALWAYS">
      <padding>
         <Insets left="10.0" right="10.0" />
      </padding></Slider>
</HBox>
